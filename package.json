{
	"name": "iac-comparisons",
	"module": "index.ts",
	"type": "module",
	"workspaces": [
		"architect",
		"cdk",
		"cdktf",
		"cloudformation",
		"pulumi/*",
		"serverless",
		"sst",
		"terraform"
	],
	"scripts": {
		"up": "docker compose up --force-recreate --yes --detach",
		"down": "docker compose down --volumes --remove-orphans && bun run --workspaces reset",
		"benchmark": "sh benchmark.sh"
	}
}
