{
	"name": "pulumi-native-local",
	"description": "Pulumi, with local backend & AWS Cloud Control provider",
	"scripts": {
		"init": "pulumi stack init local",
		"up": "pulumi up --stack local --skip-preview --refresh=false",
		"reset": "pulumi destroy --stack local --skip-preview --yes && rm -rf .pulumi"
	},
	"dependencies": {
		"@pulumi/pulumi": "3.200.0",
		"@pulumi/aws-native": "1.36.0"
	}
}
